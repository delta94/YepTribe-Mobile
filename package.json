{
  "name": "com.webtechedgeinc.yeptribe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clear": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "devtools": "react-devtools",
    "gen:graphql": "gql-gen --config codegen.yml",
    "ios": "react-native run-ios",
    "ios:beta": "(cd ios/ && fastlane beta)",
    "android": "react-native run-android",
    "android:apk": "cd android && ./gradlew assembleRelease",
    "android:install": "adb install android/app/build/outputs/apk/app-release.apk",
    "lint": "tslint --project tsconfig.json",
    "lint:staged": "lint-staged",
    "postversion": "react-native-version",
    "postinstall": "npx jetify",
    "bump:stable-release": "NEXT_STABLE=\"$(semver $npm_package_version -i release)\"; npm version $NEXT_STABLE",
    "bump:stable-major": "NEXT_STABLE=\"$(semver $npm_package_version -i major)\"; npm version $NEXT_STABLE",
    "bump:stable-minor": "NEXT_STABLE=\"$(semver $npm_package_version -i minor)\"; npm version $NEXT_STABLE",
    "bump:stable-patch": "NEXT_STABLE=\"$(semver $npm_package_version -i patch)\"; npm version $NEXT_STABLE",
    "bump:beta-release": "NEXT_BETA=\"$(semver $npm_package_version -i prerelease --preid beta)\"; npm version $NEXT_BETA",
    "bump:beta-major": "NEXT_BETA=\"$(semver $npm_package_version -i premajor --preid beta)\"; npm version $NEXT_BETA",
    "bump:beta-minor": "NEXT_BETA=\"$(semver $npm_package_version -i preminor --preid beta)\"; npm version $NEXT_BETA",
    "bump:beta-patch": "NEXT_BETA=\"$(semver $npm_package_version -i prepatch --preid beta)\"; npm version $NEXT_BETA"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/push-notification-ios": "^1.0.2",
    "@types/lodash": "^4.14.146",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/react-native-calendars": "^1.20.7",
    "@types/react-native-push-notification": "^3.0.6",
    "@types/react-native-video": "^3.1.5",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "moment-timezone": "^0.5.27",
    "react": "16.8.6",
    "react-apollo": "^3.0.0",
    "react-native": "0.60.3",
    "react-native-biometrics": "^1.6.0",
    "react-native-device-info": "^3.1.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-gravatar": "^1.0.2",
    "react-native-keyboard-accessory": "^0.1.9",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.3.1",
    "react-native-popup-menu": "^0.15.6",
    "react-native-pose": "^0.9.1",
    "react-native-push-notification": "^3.1.9",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swipeout": "^2.3.6",
    "react-native-video": "^5.0.2",
    "react-navigation": "^3.11.1",
    "react-navigation-hooks": "^1.0.1",
    "xdate": "^0.8.2"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@commitlint/cli": "^6.0.5",
    "@commitlint/config-conventional": "^6.0.4",
    "@graphql-codegen/add": "^1.5.0",
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/fragment-matcher": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-operations": "^1.5.0",
    "@graphql-codegen/typescript-react-apollo": "^1.5.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-native": "^0.57.41",
    "@types/react-native-snap-carousel": "^3.7.3",
    "@types/react-test-renderer": "^16.8.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "danger": "^4.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-okonet": "^7.0.2",
    "eslint-config-shellscape": "^2.0.2",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "eslint-plugin-relay": "^0.0.28",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-runner-eslint": "^0.6.0",
    "lint-staged": "^9.2.1",
    "metro-react-native-babel-preset": "^0.55.0",
    "nock": "^10.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "react-devtools": "^3.4.2",
    "react-native-testing-library": "^1.6.1",
    "react-native-version": "^3.0.0",
    "react-test-renderer": "16.8.6",
    "semver": "^6.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.4000",
    "typescript-eslint-parser": "^18.0.0"
  }
}
